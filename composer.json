{
    "name": "jobmetric/laravel-setting",
    "description": "This is a package for a dynamic setting across different Laravel projects.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "setting",
        "config"
    ],
    "homepage": "https://doc.jobmetric.net/package/laravel-setting",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "laravel/framework": ">=9.19"
    },
    "require-dev": {
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\Setting\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\Setting\\SettingServiceProvider"
            ],
            "aliases": {
                "Setting": "JobMetric\\Setting\\Facades\\Setting"
            }
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-setting/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-setting"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/majidmohammadian"
        }
    ]
}
